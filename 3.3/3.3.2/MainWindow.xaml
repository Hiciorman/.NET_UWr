<Window x:Class="_3._3._2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <DockPanel VerticalAlignment="Top">
            <ToolBar>
                <Button Click="Button_Click" Content="Hi"/>
                <Button Click="Button_Click_1" Content="Hello"/>
            </ToolBar>
        </DockPanel>
        <ProgressBar  Height="30" Value="50"/>
        <Expander Header="More Options">
            <StackPanel Margin="10,4,0,0">
                <CheckBox Margin="4" Content="Option 1" />
                <CheckBox Margin="4" Content="Option 2" />
                <CheckBox Margin="4" Content="Option 3" />
            </StackPanel>
        </Expander>
        <Slider  Value="3"/>
        <WebBrowser Name="myBrowser" Margin="0,0,0,0" Cursor="Arrow" Height="200"/>
        
    </StackPanel>
</Window>
